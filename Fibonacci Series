public class Fibonacci_Series {

	public static void main(String[] args) {
		
		int N = 15;
		
		int num1 = 0, num2 = 1;
        for (int i = 0; i < N; i++) {
        	if(num1>15) {
        		break;
        	}
            System.out.print(num1 + ",");
            int num3 = num1 + num2;
            num1 = num2;
            num2 = num3;
        }
			
		
		
	}

}
