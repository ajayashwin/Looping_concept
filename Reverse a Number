public class Reverse_Number {

	public static void main(String[] args) {
		
		int num = 12345678;
		
		String str_num = String.valueOf(num);
		
		String ans = "";
		
		for (int i=0;i<str_num.length();i++) 
		{
			
			ans = str_num.substring(i,i+1)+ ans;
		}
		
		System.out.println(Integer.parseInt(ans));
		

	}

}
